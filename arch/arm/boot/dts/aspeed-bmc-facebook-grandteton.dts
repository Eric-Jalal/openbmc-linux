		compatible = "iio-hwmon";
		io-channels = <&ads1015_vpdb 0>, <&ads1015_vpdb 1>, <&ads1015_vpdb 2>, <&ads1015_vpdb 3>,
			<&ads1015_vpdb 4>, <&ads1015_vpdb 5>, <&ads1015_vpdb 6>, <&ads1015_vpdb 7>;
