/dts-v1/;

#include "ast2600-facebook-netbmc-common.dtsi"
/ {
        model = "Facebook Sandia BMC";
        compatible = "facebook,sandia-bmc", "aspeed,ast2600";
};

&mac3 {
        status = "okay";
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_rgmii4_default>;
        phy-mode = "rgmii";
        fixed-link {
                speed = <1000>;
                full-duplex;
        };
};
